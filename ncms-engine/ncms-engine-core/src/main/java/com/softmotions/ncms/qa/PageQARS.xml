<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.softmotions.ncms.qa.PageQARS">

    <cache-ref namespace="com.softmotions.ncms.db.cache.asm"/>

    <select id="selectCPBatch" resultType="long">
        SELECT
            asm.id
        FROM ASMS AS asm
        WHERE
            asm.qa_check_date IS NULL
            OR asm.mdate > asm.qa_check_date
        ORDER BY asm.mdate ASC
        LIMIT #{limit}
    </select>

</mapper>