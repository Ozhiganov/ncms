<?xml version="1.0" encoding="utf-8" ?>

<configuration>
    <ncms-prefix>/ncms</ncms-prefix>

    <jar-web-resources>
        <resource>
            <path-prefix>/</path-prefix>
            <options>/ncms-engine-qx/ncms</options>
        </resource>
    </jar-web-resources>

    <mybatis config="com/softmotions/ncms/db/myibatis-config.xml">
        <property name="JDBC.driver" value="${ncms.JDBC.driver}"/>
        <property name="JDBC.url" value="${ncms.JDBC.url}"/>
        <property name="JDBC.username" value="${ncms.JDBC.username}"/>
        <property name="JDBC.password" value="${ncms.JDBC.password}"/>
    </mybatis>

</configuration>