<ul>
    #foreach(Tree n in asm("menu"))
    <li><a href="">${n.name}</a>
        #if(n.hasChildren)
        <div class="slide-holder">
            <div class="slide-frame">
                <ul>
                    #foreach(Tree c in n)
                    <li><a href="$!{c.link}">${c.name}</a></li>
                    #end
                </ul>
            </div>
        </div>
        #end
    </li>
    #end
</ul>