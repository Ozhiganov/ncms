<!DOCTYPE html>
<html xmlns:og="http://ogp.me/ns#">
<head>
    $!{asm("head")}
</head>
<body>
<div id="wrapper">
    <div class="w1">
        <header id="header">
            ${include("inc/header.httl")}
        </header>
        <div id="main">
            ${include("inc/breadcrumbs.httl")}
            <div id="sitemap" class="wiki">
                <ul class="dynamic">
                    <li class="open node">
                        <span></span>
                        ${page().hname}
                        <ul>${include("inc/sitemap_level_items.httl")}</ul>
                    </li>
                </ul>
            </div>
            ${include("inc/medialine.httl")}
        </div>
        ${include("inc/soclikes.httl")}
        <footer id="footer">
            ${include("inc/footer.httl")}
        </footer>
    </div>
</div>
$!{include("inc/scripts.httl")}
<script type="text/javascript">
    window.initSiteMap && $(function(){
        initSiteMap("$!{page().link}");
    });
</script>
</body>
</html>
