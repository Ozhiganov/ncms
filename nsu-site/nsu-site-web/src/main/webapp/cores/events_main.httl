<!DOCTYPE html>
<html xmlns:og="http://ogp.me/ns#">
<head>
    $!{asm("head")}
</head>
<body>
<div id="wrapper">
<div class="w1">
<header id="header">
    ${include("inc/header.httl")}
</header>
<div id="main" class="events-page">
${include("inc/breadcrumbs.httl")}
<div class="filter-holder">
    <h1>Календарь событий</h1>
    <ul class="tabset-list">
        #if(events_past == true)
        <li><a href="$!{page().link}">Грядущие</a></li>
        <li class="active"><a>Прошедшие</a></li>
        #else
        <li class="active"><a>Грядущие</a></li>
        <li><a href="$!{page().link}?past">Прошедшие</a></li>
        #end
    </ul>
</div>
<ul class="event-list tab active">
    $!{asm("news_list")}
</ul>
<a class="fetch-more"><span>Загрузить еще</span></a>
${include("inc/partners.httl")}
</div>
${include("inc/soclikes.httl")}
<footer id="footer">
    ${include("inc/footer.httl")}
</footer>
</div>
</div>
$!{include("inc/scripts.httl")}
<script>
    window.initRemember && initRemember();
    window.initEventsMain && initEventsMain('$!{page().link}?${ifTrue(events_past == true, "past")}', $!{news_limit});
    window.initScroll && $(function(){
        initScroll('ul.tabset-list > li > a', 'nsu:events');
    });
</script>
</body>
</html>
